alias vi 'nvim'
alias clip 'pbcopy'
alias denops='deno run -A --no-lock ~/.cache/dein/repos/github.com/vim-denops/denops.vim/denops/@denops-private/cli.ts'

alias cp 'cp -i'
alias mv 'mv -i'
alias rm 'rm -i'
alias stat 'stat -x'

alias ... 'cd ../../'
alias .... 'cd ../../../'
alias ..... 'cd ../../../../'

abbr --add :q 'exit'
abbr --add :wq 'exit'
abbr --add :wqa 'exit'
abbr --add :qa 'exit'
abbr --add :w ''

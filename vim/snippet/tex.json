{
  "abs": {
    "prefix": "abs",
    "body": "\\abs{$0}"
  },
  "approx": {
    "prefix": "approx",
    "body": "\\approx"
  },
  "begin": {
    "prefix": "begin",
    "body": [
      "\\begin{$1}",
      "  $0",
      "\\end{$1}"
    ]
  },
  "cdot": {
    "prefix": ["cdot", "dot"],
    "body": "\\cdot "
  },
  "ce" :{
    "prefix": "ce",
    "body": "\\ce{$0}"
  },
  "cite": {
    "prefix": "cite",
    "body": "\\cite{$0}"
  },
  "cos": {
    "prefix": "cos",
    "body": "\\cos{$0}"
  },
  "equation": {
    "prefix": "equation",
    "body": [
      "\\begin{equation}",
      "  $0",
      "\\end{equation}"
    ]
  },
  "figure": {
    "prefix": "figure",
    "body": [
      "\\begin{figure}[H]",
      "  \\centering",
      "  \\includegraphics[width=10truecm]{./fig/$1}",
      "  \\caption{$2}",
      "\\end{figure}",
      "$0"
    ]
  },
  "frac": {
    "prefix": "frac",
    "body": "\\frac{$1}{$0}"
  },
  "item": {
    "prefix": "item",
    "body": "\\item "
  },
  "itemize": {
    "prefix": ["itemize", "list"],

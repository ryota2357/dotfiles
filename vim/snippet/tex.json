  "table": {
    "prefix": "table",
    "body": [
      "\\begin{table}[H]",
      "  \\begin{center}",
      "    \\caption{$1}",
      "    \\label{表:$1}",
      "    \\begin{tabular}{$2:lcr...}",
      "      \\hline",
      "        $0",
      "      \\hline",
      "    \\end{tabular}",
      "  \\end{center}",
      "\\end{table}"
    ]
  },
  "thebibliography": {
    "prefix": "thebibliography",
    "body": [
      "%!!!> ## 参考文献",
      "\\begin{thebibliography}{9}",
      "  \\bibitem{$1} $0",
      "\\end{thebibliography}"
    ]
  },
